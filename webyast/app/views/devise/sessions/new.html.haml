%h2 Sign in

- logger.error(resource_name.class)

/ FIXME session_path and resource_name did not work here
= form_for(:user, :url => account_session_path) do |f| 
  = f.label :login
  %br
  = f.text_field :login
  %br
  = f.label :password
  %br
  = f.password_field :password
  %br
  - if devise_mapping.rememberable? 
    = f.check_box :remember_me
    = f.label :remember_me
  %br
  = f.submit "Sign in"

= render :partial => "devise/shared/links"
