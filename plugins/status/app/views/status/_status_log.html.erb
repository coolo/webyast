<table  class="list" cellpadding="0" cellspacing="20">
  <tr>
     <td class="first">
        <%= link_to image_tag("go-top2.png", :border => 0), {:action => "ajax_log_custom", :id => id, :lines => lines, :pos_begin =>1}, :class => "loglink", "data-target" => log_id(id) %>
     </td>
     <td>
        <%= link_to image_tag("go-up2.png", :border => 0), {:action => "ajax_log_custom", :id => id, :lines => lines, :pos_begin =>(position-lines)}, :class => "loglink", "data-target" => log_id(id) %>
     </td>
     <td>
        
        <%= link_to image_tag("go-down2.png", :border => 0), {:action => "ajax_log_custom", :id => id, :lines => lines, :pos_begin =>(position+lines)}, :class => "loglink", "data-target" => log_id(id) %>
     </td>
     <td class="last">
        <%= link_to image_tag("go-bottom2.png", :border => 0), {:action => "ajax_log_custom", :id => id, :lines => lines, :pos_begin =>0}, :class => "loglink", "data-target" => log_id(id) %>
     </td>
  </tr>
</table>

<pre><%= content %></pre>


